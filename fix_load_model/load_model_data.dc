global function merge_tables(tables) do
    if len(tables) == 0 do
        return null
    endif
    
    # –ü–æ–ª—É—á–∞–µ–º –∑–∞–≥–æ–ª–æ–≤–∫–∏ –∏–∑ –ø–µ—Ä–≤–æ–π —Ç–∞–±–ª–∏—Ü—ã
    local headers = table_headers(tables[0])
    
    # –°–æ–±–∏—Ä–∞–µ–º –≤—Å–µ —Å—Ç—Ä–æ–∫–∏ –∏–∑ –≤—Å–µ—Ö —Ç–∞–±–ª–∏—Ü
    local all_rows = []
    
    for table in tables do
        # –ò—Ç–µ—Ä–∏—Ä—É–µ–º—Å—è –ø–æ —Å—Ç—Ä–æ–∫–∞–º —Ç–∞–±–ª–∏—Ü—ã (–∫–∞–∂–¥–∞—è —Å—Ç—Ä–æ–∫–∞ - —ç—Ç–æ –º–∞—Å—Å–∏–≤)
        for row in table do
            all_rows = push(all_rows, row)
        next row
    next table
    
    # –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–¥–∏–Ω–µ–Ω–Ω—É—é —Ç–∞–±–ª–∏—Ü—É
    return table_create(all_rows, headers)
endfunction

# –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ñ–æ—Ä–º–∞—Ç–∏—Ä–æ–≤–∞–Ω–∏—è –º–µ—Å—è—Ü–∞ (–¥–æ–±–∞–≤–ª—è–µ—Ç –≤–µ–¥—É—â–∏–π –Ω–æ–ª—å)
global function format_month(month) do
    if month < 10 do
        return "0" + month
    else
        return "" + month
    endif
endfunction

# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø—Ä–æ–≤–µ—Ä–∫–∏ –Ω–∞–ª–∏—á–∏—è —Ñ–∞–π–ª–∞ –≤ —Å–ø–∏—Å–∫–µ
global function file_exists(files, filename) do
    for file in files do
        if file == filename do
            return true
        endif
    next file
    return false
endfunction

