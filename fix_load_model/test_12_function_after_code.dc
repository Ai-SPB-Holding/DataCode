# Код перед функцией
print("Код перед функцией")
global test_var = "test"

# Функция с вложенными циклами
global function test_func() do
    local arr = [1, 2]
    for x in arr do
        for y in arr do
            print(x, y)
        next y
    next x
    return "ok"
endfunction

# Цикл for с try внутри
global test_array = ["a"]

for item in test_array do
    try
        print("  Item: ", item)
    catch e
        print("  Error: ", e)
    endtry
next item

