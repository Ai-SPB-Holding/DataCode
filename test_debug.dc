print("Testing nested loops with global variables:")
global result = []
for i in range(2) do
    print("Outer loop i =")
    print(i)

    global inner_array = []
    for j in range(2) do
        print("  Inner loop j =")
        print(j)
        global item = str(i) + '_' + str(j)
        global inner_array = push(inner_array, item)
    forend

    print("After inner loop, inner_array =")
    print(inner_array)
    global result = push(result, inner_array)
    print("Result after push:")
    print(result)
forend
print("Final result:")
print(result)
print("Result length:")
print(length(result))
