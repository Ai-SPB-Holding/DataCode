print('===================================')

global function find_max_with_index(arr) do
    global max_value = null
    global max_index = -1

    for pair in enum(arr) do
        global idx = pair[0]
        global val = pair[1]

        if isinstance(val, 'number') do
            if max_value == null do
                global max_value = val
                global max_index = idx
            else
                if val > max_value do
                    global max_value = val
                    global max_index = idx
                endif
            endif
        endif
    forend

    return [max_index, max_value]
endfunction

