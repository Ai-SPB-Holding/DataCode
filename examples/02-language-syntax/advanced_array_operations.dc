global function create_matrix(rows, cols, initial_value) do
    local matrix = array_builder(rows)
    
    for i in range(0, rows) do
        local row = bulk_create(cols, initial_value)
        local matrix = push(matrix, row)
    next i
    
    return matrix
endfunction

