global function process_data_chunks(chunk1, chunk2, chunk3) do
    # –û–±—ä–µ–¥–∏–Ω—è–µ–º –≤—Å–µ —á–∞–Ω–∫–∏
    local combined = extend(chunk1, chunk2)
    combined = extend(combined, chunk3)
    
    # –†–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–µ–º –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –≤ –æ–±—Ä–∞—Ç–Ω–æ–º –ø–æ—Ä—è–¥–∫–µ
    local reversed = reverse(combined)
    
    # –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–π –º–∞—Å—Å–∏–≤ —Å —Ä–µ–∑—É–ª—å—Ç–∞—Ç–∞–º–∏
    local results = array_builder(len(reversed))
    
    for item in reversed do
        # –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º –∫–∞–∂–¥—ã–π —ç–ª–µ–º–µ–Ω—Ç
        local processed = item * 2
        local results = push(results, processed)
    next item
    
    return reverse(results)  # –í–æ–∑–≤—Ä–∞—â–∞–µ–º –≤ –∏—Å—Ö–æ–¥–Ω–æ–º –ø–æ—Ä—è–¥–∫–µ
endfunction

global chunk1 = [1, 2, 3]
global chunk2 = [4, 5, 6]
global chunk3 = [7, 8, 9]

global processed = process_data_chunks(chunk1, chunk2, chunk3)
print('–ò—Å—Ö–æ–¥–Ω—ã–µ —á–∞–Ω–∫–∏:')
print('  chunk1 =', chunk1)
print('  chunk2 =', chunk2)
print('  chunk3 =', chunk3)
print('–û–±—Ä–∞–±–æ—Ç–∞–Ω–Ω—ã–µ –¥–∞–Ω–Ω—ã–µ:', processed)
print()

