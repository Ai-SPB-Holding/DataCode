global function max_value(a, b) do
    if a > b do
        return a
    else
        return b
    endif
endfunction

global function abs_value(num) do
    if num < 0 do
        return -num
    else
        return num
    endif
endfunction

global function classify_number(num) do
    if num > 0 do
        return 'positive'
    else
        if num < 0 do
            return 'negative'
        else
            return 'zero'
        endif
    endif
endfunction

# –¢–µ—Å—Ç–∏—Ä—É–µ–º —Ñ—É–Ω–∫—Ü–∏–∏
