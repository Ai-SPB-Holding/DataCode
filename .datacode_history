#V2
throw "Test error"
global test = "Test error"
print("Hello world")
throw "Test error"
try
throw "Inner error"
catch inner_error
global inner_caught = true
throw "Outer error"
endtry
catch outer_error
global outer_caught = true
endtry
global outer_caught = false
global inner_caught = false
try
throw "Inner error"
catch inner_error
global inner_caught = true
throw "Outer error"
endtry
catch outer_error
global outer_caught = true
endtry
print("inner_caught:", inner_caught)
print("outer_caught:", outer_caught)
